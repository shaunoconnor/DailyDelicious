{
  "manifest_version": 2,
  "options_page": "options.html",
  "name": "Daily Delicious",
  "description": "An extension to load bookmarks tagged with daily or what ever you want.",
  "version": "1.0",
  "content_security_policy": "script-src 'self' https://feeds.delicious.com/; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  }
}